generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "postgresql"
  url      = env("DATABASE_URL")
}

model Game {
  id String @id @default(uuid())
  title String @unique
  coverimageurl String
  description String
  year Int
  imdbscore Int
  traileryoutubeurl String
  gameplayyoutubeurl String
  createdAt DateTime @default(now())
  updatedAt DateTime @updatedAt
}

